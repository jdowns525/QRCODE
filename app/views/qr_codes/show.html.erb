<h1>QR Code Information</h1>

<!-- Add the generated QR code -->
<div>
  <%= raw generate_qr_code_svg(@qr_code_info) %>
</div>

<p>
  <strong>Name:</strong>
  <%= @qr_code_info.first_name %> <%= @qr_code_info.last_name %>
</p>

<p>
  <strong>City:</strong>
  <%= @qr_code_info.city %>
</p>

<p>
  <strong>Address:</strong>
  <%= @qr_code_info.address %>
</p>

<p>
  <strong>ZIP:</strong>
  <%= @qr_code_info.zip %>
</p>

<p>
  <strong>State:</strong>
  <%= @qr_code_info.state %>
</p>

<p>
  <strong>Job Place:</strong>
  <%= @qr_code_info.job_place %>
</p>

<p>
  <strong>Job Description:</strong>
  <%= @qr_code_info.job_description %>
</p>

<p>
  <strong>GitHub:</strong>
  <%= link_to 'Your GitHub Profile', 'https://github.com/your_username', target: '_blank' %>
</p>

<p>
  <strong>Personal Website:</strong>
  <%= link_to 'Your Personal Website', 'https://yourpersonalwebsite.com', target: '_blank' %>
</p>
